{
    "name": "blackbricksoftware/composer-gravitywiz",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "This composer plugin enables installation of Gravity Perks WordPress plugins from Gravity Wiz. Base on Composer GravityForms (https://github.com/PiotrPress/composer-gravitywiz).",
    "homepage": "https://github.com/blackbricksoftware/composer-gravitywiz/",
    "authors": [
        {
            "name": "David Hayes",
            "homepage": "https://blackbricksoftware.com"
        }
    ],
    "keywords": [
        "composer", "composer plugin", "gravityforms", "gravitywiz", "gravityperks", "wordpress", "plugin", "addon", "addons", "package", "packages", "php"
    ],
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^2.0",
        "piotrpress/streamer": "^1.0"
    },
    "extra": {
        "class": "BlackBrickSoftware\\Composer\\GravityWiz\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "BlackBrickSoftware\\Composer\\GravityWiz\\": "src/"
        },
        "classmap": [
            "src"
        ]
    }
}